{
  "name": "agentpay-api",
  "type": "module",
  "version": "0.1.0",
  "description": "AgentPay API - middleware between AI agents and Stripe",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
