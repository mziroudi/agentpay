    "fastify": "^4.28.0",
    "ioredis": "^5.4.0",
    "jsonwebtoken": "^9.0.0",
    "pg": "^8.12.0",
    "resend": "^3.0.0",
    "stripe": "^14.0.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.0",
    "@types/node": "^20.0.0",
    "@types/pg": "^8.11.0",
    "@types/uuid": "^9.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0",
